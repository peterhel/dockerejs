upstream <%= Config.Labels.hostname %> {
  server <%= NetworkSettings.Ports['80/tcp'][0].HostIp %>:<%= NetworkSettings.Ports['80/tcp'][0].HostPort %>;
}

server {
    server_name <%= Config.Labels.hostname %>.example.com;
    location / {
        proxy_pass http://<%= Config.Labels.hostname %>;
    }
}
